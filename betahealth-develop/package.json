  "devDependencies": {
    "browser-sync": "~2.18.6",
    "chai": "~3.5.0",
    "chai-as-promised": "~6.0.0",
    "chai-string": "~1.3.0",
    "codeclimate-test-reporter": "~0.4.0",
    "eslint": "~3.13.1",
    "eslint-config-airbnb-base": "~11.0.1",
    "eslint-plugin-import": "~2.2.0",
    "eslint-plugin-mocha": "~4.8.0",
    "gulp-eslint": "~3.0.1",
    "gulp-nodemon": "~2.2.1",
    "gulp-sass-lint": "~1.3.2",
    "istanbul": "~0.4.5",
    "mkdirp": "~0.5.1",
    "mocha": "~3.2.0",
    "mock-fs": "~3.12.1",
    "progress": "~1.1.8",
    "proxyquire": "~1.7.10",
    "rewire": "~2.5.2",
    "selenium-standalone": "~5.9.1",
    "sinon": "~1.17.7",
    "sinon-chai": "~2.8.0",
    "sinon-stub-promise": "~4.0.0",
    "ua-parser-js": "~0.7.12",
    "wdio-mocha-framework": "~0.5.8",
    "webdriverio": "~4.6.1"
  },
  "pre-commit": [
    "test:unit",
    "lint"
  ]
